---
import { getCollection } from "astro:content";
import type { CollectionEntry } from "astro:content";

import NotesOverviewLayout from "@layouts/NotesOverviewLayout.astro";

const notes: CollectionEntry<"notes">[] = await getCollection("notes", ({ data }) => {
    return data.isPublic;
});
---

<NotesOverviewLayout
    title="My Notes"
    notes={notes}
/>
